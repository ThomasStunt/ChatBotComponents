{
  "name": "chatbot",
  "version": "0.1.0",
  "scripts": {
    "start": "node ./server",
    "clean": "bsb -clean-world",
    "build": "bsb -make-world && parcel build index.html",
    "watch": "concurrently -k -n bsb,jest -c magenta,green,blue \"bsb -make-world -w\" \"jest --watch\" \"parcel watch index.html\"",
    "byte": "bsb -backend",
    "test": "npm run build && jest --coverage",
    "parcel": "parcel index.html"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reason-react": ">=0.4.0",
    "@glennsl/bs-json": "^3.0.0",
    "bs-fetch": "^0.3.1",
    "bs-json": "^1.0.1",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "helmet": "^3.15.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.7",
    "bs-platform": "^4.0.18",
    "bsb-js": "^1.1.7",
    "concurrently": "^4.1.0",
    "parcel-bundler": "^1.11.0"
  }
}
